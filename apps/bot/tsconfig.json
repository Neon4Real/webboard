{
    "extends": "config/base.json",
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "dist",
        "esModuleInterop": true,
        "module": "es2020",
        "moduleResolution": "nodenext",
        "target": "ES2022",
        "strictNullChecks": true,
        "noImplicitThis": true,
        "paths": {
            "@/*": ["./*"]
        }
    },
    "exclude": ["node_modules"]
}
